<!-- Mobile Header End -->
<!-- SM Header Start -->
    <header class="hidden-xs">
      <div class="container">
        <div class="row">
          <div class="col-sm-2">
              <a href="/"><img src="/img/logoTwo.png" alt="Ale Nomad Logo"></a>
          </div>
          <div class="col-sm-8">
              <nav>
                <ul>
                  <li><a href="/find_beer">Find Beers</a></li>
                  <li><a href="/find_brew">Find Breweries</a></li>
                  <li><a href="/userprofile">Profile</a></li>
                </ul>
              </nav>
          </div>
          <div class="col-sm-2 topRowButtons">
            <button class="btn btn-default pull-right"><a href="signup">Signup</a></button>
          </div>
        </div>
      </div>
    </header>
<!-- SM Header End -->


  <div id="aleNomad"></div>

  <div class="container">
    <div class="row footer text-center">
      <div class="text-center hidden-xs">
        <strong>The Developers</strong>
      </div>
        <div class="col-sm-8 col-sm-offset-2">
